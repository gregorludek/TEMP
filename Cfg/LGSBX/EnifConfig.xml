    <DateToEx>20300101</DateToEx>
    <FxClasses>
      <FxClass>EnifNaosC1.CFxListManager,EnifNaosC164 64</FxClass>
      <FxClass>EnifNaosAthena.CFxListManager,EnifNaosAthena64 64</FxClass>
